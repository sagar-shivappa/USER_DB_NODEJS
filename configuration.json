{
  "version": "1.0",
  "default_open_files": [
   "routes/userRoutes.js"
  ],
  "read_only_files": [
    "/tests/App.test.js",
    ".gitignore",
    "configuration.json"
  ],
  "buttons": {
    "Install Packages": {
      "command": "npm install"
    },
    "Start MongoDB": {
      "command": "mongod --dbpath /var/lib/mongo --logpath /var/log/mongodb/mongod.log"
    },
    "Start API Server": {
      "command": "npm start"
    },
    "Run Test Cases": {
      "command": "npm test"
    }
  },
  "scoring": {
    "command": "npm install && npm run test",
    "files": {
      "junit.xml": "Node & Express"
    }
  },
  "post_start": {
    "commands": [
      "npm install",
      "mongod --dbpath /var/lib/mongo --logpath /var/log/mongodb/mongod.log"
    ]
  },
  "preview_port": 8000
}
